{
  "name": "simple-performance-statistics",
  "version": "0.0.1",
  "description": "simple static of performance metrics",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
